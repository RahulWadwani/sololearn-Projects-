   "source": [
    "string = str(input(\"Enter you symbol message: \"))\n",
    "decoded = ''\n",
    "for i in string:\n",
    "    if ((ord(i)>=65 and ord(i)<=90) or (ord(i)>=97 and ord(i)<=122) or (ord(i)==32)):\n",
    "        decoded += i\n",
    "print(decoded)"
   ]
  }
